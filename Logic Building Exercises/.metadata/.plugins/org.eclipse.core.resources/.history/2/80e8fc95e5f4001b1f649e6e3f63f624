package com.pattern;

public class Pattern3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i=0; i<5; i++) {
			for(int j=i; j<5;j++) {
				if(i==0 && i==4) {
					System.out.println("******");
				}else {
					System.out.println("*    *");
				}
			}
		}
	}

}
