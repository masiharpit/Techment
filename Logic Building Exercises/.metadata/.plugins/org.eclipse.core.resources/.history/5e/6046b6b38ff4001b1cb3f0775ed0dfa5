package practise_exercises;

public class Exercise12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i, j, k;
		for(i = 0; i<6; i++) {
			for(j=i; j<6;j++) {
				System.out.print(" ");
			}
			for(k=5; k>0;k--) {
				System.out.print(k);
			}
			System.out.println();
		}
	}

}
